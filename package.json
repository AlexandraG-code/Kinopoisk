{
	"name": "react-lessons",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"format": "prettier --write . --log-level=debug",
		"format:dir": "prettier --write  --log-level=debug",
		"generate:api": "swagger-typescript-api -p https://api.kinopoisk.dev/documentation-json  -o src/shared/types -n types.ts",
		"generate:api-from-file": "swagger-typescript-api -p ./public/swaggerfile.json  -o src/shared/types -n typesFile.ts --split",
		"generate:api-from-dev": "swagger-typescript-api -p http://localhost:5001/api/swagger/v1/swagger.json  -o src/shared/types -n api.ts --split",
		"open-api": "openapi-typescript http://localhost:5001/api/swagger/v1/swagger.json --output src/shared/types/api.ts --split",
		"gen": "swagger-typescript-api -p http://localhost:5001/api/swagger/v1/swagger.json  -o src/shared/types -n apiTest.ts --modular  --axios --route-types --no-client   --type-prefix swagger"
	},
	"dependencies": {
		"@ant-design/icons": "^5.5.1",
		"@reduxjs/toolkit": "^2.3.0",
		"antd": "^5.21.3",
		"axios": "^1.7.7",
		"babel-plugin-module-resolve": "^1.7.0",
		"clsx": "^2.1.1",
		"openapi-typescript": "^7.5.2",
		"prettier": "^3.5.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-redux": "^9.1.2",
		"react-router-dom": "^6.27.0",
		"redux-devtools-extension": "^2.13.9",
		"swagger-typescript-api": "^13.0.22",
		"ts-node": "^10.9.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.11.1",
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@types/node": "^22.7.5",
		"@types/react": "^18.3.10",
		"@types/react-dom": "^18.3.0",
		"@types/rollup": "^0.54.0",
		"@vitejs/plugin-react": "^4.3.4",
		"eslint-plugin-react-hooks": "^5.1.0-rc.0",
		"eslint-plugin-react-refresh": "^0.4.12",
		"globals": "^15.9.0",
		"sass": "^1.85.0",
		"sass-embedded": "^1.79.5",
		"typescript": "^5.6.3",
		"vite": "^6.1.0"
	}
}
